<%@ Control Language="VB" AutoEventWireup="false" CodeFile="_viewPort.ascx.vb" Inherits="SBS_Shared_Layouts_Common_viewPort" %>

<%--<script runat="server">
    Public Function InitViewport() As String
        If Request.Cookies("noneresponsive") Is Nothing Then
            Return "<meta name='viewport' content='width=device-width, initial-scale=1.0'>"
        End If
        
        Return String.Empty
    End Function
</script>

<%= InitViewport() %>--%>